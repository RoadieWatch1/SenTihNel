{
  "expo": {
    "name": "Device Health",
    "slug": "device-health",
    "scheme": "sentihnel",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sentihnel.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.sentihnel.app",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.INTERNET",
        "android.permission.CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SenTihNel to access your camera for security.",
          "microphonePermission": "Allow SenTihNel to hear the wake word."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "SenTihNel needs your location in the background to track you during an emergency.",
          "locationAlwaysPermission": "SenTihNel needs your location in the background to track you during an emergency.",
          "locationWhenInUsePermission": "SenTihNel needs your location to track you during an emergency.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true 
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb96224a-20be-40f3-9be5-087dc09c2efc"
      }
    }
  }
}